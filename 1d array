#include <stdio.h>
int main() {
    int arr[10];   
    int i, n;
    printf("Enter number of elements (max 10): ");
    scanf("%d", &n);
    printf("Enter %d integers:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("The array elements are:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
 return 0;
}
